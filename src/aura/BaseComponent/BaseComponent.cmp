<aura:component description="This component handles all dynamic creation of components within a community"
                implements="forceCommunity:availableForAllPageTypes"
                extends="c:Util"
                controller="BaseComponentCtrl">

    <aura:handler name="init" value="{!this}" action="{!c.init}" />
    <!--4d pocket holds all settings and relevant data JSON objects across the community-->
    <aura:attribute name="pocket4d" type="Object" default="{}" />

    <span>This is the base component</span>
</aura:component>
